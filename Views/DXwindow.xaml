            <!-- íƒ­ 2: ì •ë³´ -->
            <TabItem Header="â„¹ï¸ Info">
                <ScrollViewer VerticalScrollBarVisibility="Auto">
                    <StackPanel Margin="12">
                        <TextBlock Text="DXTnavis" FontSize="22" FontWeight="Bold"
